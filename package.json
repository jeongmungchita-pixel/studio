{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage --reporter=dot --silent --reporter=json --outputFile=.coverage-last.json",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "cleanup:analyze": "node src/scripts/cleanup-analyzer.js",
    "cleanup:imports": "node src/scripts/cleanup-imports.js",
    "cleanup:console": "node src/scripts/remove-console-logs.js",
    "cleanup:all": "npm run cleanup:imports && npm run cleanup:console",
    "clear:build": "rm -rf .next out",
    "clear:modules": "rm -rf node_modules package-lock.json",
    "clear:all": "npm run clear:build && npm run clear:modules",
    "reset:database": "node scripts/reset-database.js",
    "audit:routes": "node src/scripts/route-audit.js",
    "audit:security": "npm audit",
    "audit:all": "npm run audit:routes && npm run audit:security",
    "quality:check": "npm run typecheck && npm run lint && npm run test",
    "quality:fix": "npm run cleanup:all && npm run lint:fix",
    "quality:gate": "node src/scripts/quality-gate.js",
    "health:check": "node src/scripts/project-health.js",
    "health:monitor": "node src/scripts/project-health.js --monitor",
    "monitor:full": "node src/scripts/automated-monitoring.js",
    "security:scan": "node src/scripts/check-secrets.js",
    "maintenance:daily": "npm run cleanup:all && npm run security:scan && npm run typecheck",
    "maintenance:weekly": "npm run maintenance:daily && npm run monitor:full && npm audit",
    "pre-deploy": "npm run maintenance:weekly && npm run test:coverage && npm run build && npm run quality:gate",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:all": "firebase deploy",
    "reset:firestore": "tsx scripts/reset-firestore.ts",
    "validate:schema": "tsx scripts/validate-firestore-schema.ts",
    "fix:schema": "tsx scripts/fix-firestore-schema.ts",
    "simulate": "tsx scripts/simulate-full-workflow.ts"
  },
  "dependencies": {
    "@genkit-ai/google-genai": "^1.22.0",
